    "bump-file": "1.0.0",
    "chalk": "2.2.0",
    "cpy": "6.0.0",
    "debug": "3.1.0",
    "github": "11.0.0",
    "globby": "6.1.0",
    "inquirer": "3.3.0",
    "lodash": "4.17.4",
    "ora": "1.3.0",
