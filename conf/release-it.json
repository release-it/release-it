{
  "non-interactive": false,
  "dry-run": false,
  "verbose": false,
  "pkgFiles": ["package.json"],
  "increment": "patch",
  "preReleaseId": null,
  "buildCommand": null,
  "safeBump": true,
  "beforeChangelogCommand": null,
  "changelogCommand": "git log --pretty=format:\"* %s (%h)\" [REV_RANGE]",
  "requireCleanWorkingDir": true,
  "requireUpstream": true,
  "src": {
    "commit": true,
    "commitMessage": "Release ${version}",
    "commitArgs": "",
    "tag": true,
    "tagName": "${version}",
    "tagAnnotation": "Release ${version}",
    "tagArgs": "",
    "push": true,
    "pushArgs": "",
    "pushRepo": "origin",
    "beforeStartCommand": null,
    "afterReleaseCommand": null,
    "addUntrackedFiles": false
  },
  "npm": {
    "publish": true,
    "publishPath": ".",
    "tag": "latest",
    "private": false,
    "access": null,
    "otp": null
  },
  "github": {
    "release": false,
    "releaseName": "Release ${version}",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": null,
    "host": null,
    "timeout": 0,
    "proxy": null
  },
  "dist": {
    "repo": false,
    "stageDir": ".stage",
    "baseDir": "dist",
    "files": ["**/*"],
    "pkgFiles": null,
    "commit": true,
    "commitMessage": "Release ${version}",
    "commitArgs": "",
    "tag": true,
    "tagName": "${version}",
    "tagAnnotation": "Release ${version}",
    "tagArgs": "",
    "push": true,
    "pushArgs": "",
    "beforeStageCommand": null,
    "afterReleaseCommand": null,
    "addUntrackedFiles": null,
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  },
  "prompt": {
    "src": {
      "status": false,
      "commit": true,
      "tag": true,
      "push": true,
      "release": true,
      "publish": true
    },
    "dist": {
      "status": false,
      "commit": true,
      "tag": false,
      "push": true,
      "release": false,
      "publish": false
    }
  }
}
